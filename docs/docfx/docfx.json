{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/TwitchNET.Core/TwitchNET.Core.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ],
          "cwd": "../.."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "guides/**.md",
          "guides/**/**.md",
          "guides/toc.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "Twitch .NET Documentation",
      "_appFooter": "<span>Copyright &copy; Naendo</span>",
      "_enableSearch": "true"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/discordfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
